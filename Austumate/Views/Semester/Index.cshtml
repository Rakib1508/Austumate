@model IEnumerable<Austumate.Models.SemesterModel>

@{
    ViewBag.Title = "Index";
}

<h2 class="mt-5 mb-4">All Semesters</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Session)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Session)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SemesterID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SemesterID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SemesterID })
        </td>
    </tr>
}

</table>
